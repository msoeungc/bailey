  <%- include("partials/head"); -%>
  </head>

  <body>
    <main class="main-grid">
      <div class="header">
        <%- include("partials/header"); -%>
      </div>
      <div class="main">
        <section class="header-sec">
          <div class="home-background"></div>
          <div class="home-background2"></div>
          <div class="header-flex-container">
            <div class="transparent-bg-home">
              <div class="title-content">
                <h1 class="title-heading">See cute pictures of my dog, Bailey and my cat, Scout! </h1>
                <p class="subtext-heading">
                  <a href="/signup">Sign up</a> with your email for more Bailey & Scout content! (Example site)
                </p>
              </div>
            </div>
          </div>
        </section>
        <section class="color-bg vid-1-cont">
          <div class="vid-container">
            <video class="home-vid scout-vid" id="background-video" autoplay loop muted poster="">
              <source src="/images/vidbg/2.mp4" type="video/mp4">
            </video>
            <div class="vid-header">
              <h2>Learn more about Bailey and Scout</h2>
              <a href="/aboutus">
                <input class="about-btn" type="submit" value="Learn more">
              </a>
            </div>
          </div>
        </section>

        <section class="color-bg carousel">
          <div class="carousel-sec">
            <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
              <div class="carousel-wrap">
                <div class="carousel-inner">
                  <!--1150px-->
                  <div class="carousel-item active">
                    <img src="/images/baileyCarousel1.jpg" class="carousel-img" alt="...">
                  </div>
                  <div class="carousel-item">
                    <img src="/images/scoutCarousel1.jpg" class="carousel-img" alt="...">
                  </div>
                  <div class="carousel-item">
                    <img src="/images/baileyCarousel2.jpg" class="carousel-img" alt="...">
                  </div>
                  <div class="carousel-item">
                    <img src="/images/scoutChair.jpg" class="carousel-img" alt="...">
                  </div>
                  <div class="carousel-item">
                    <img src="/images/scoutPawCross.jpg" class="carousel-img" alt="...">
                  </div>
                </div>
              </div>
              <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
              </a>
              <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
              </a>
            </div>
          </div>
        </section>

        <div class="vid-container">
          <video class="home-vid bails-vid" id="background-video" autoplay loop muted poster="">
            <source src="/images/vidbg/1.mp4" type="video/mp4">
          </video>
          <div class="vid-header">
            <h2>Bailey showing off his catching abilities</h2>
            <a href="/signup">
              <input class="signup-btn" type="submit" value="Sign up">
            </a>
          </div>
        </div>

        <section class="color-bg home-img-container">
          <section class="home-img-sec">
            <div class="img-flex-l">
              <h2 class="home-des">Bailey hanging out with his best friend, Nori!</h2>
              <div class="img-wrap home-imgs">
                <div class="img"></div>
              </div>
            </div>
          </section>
          <section class="home-img-sec">
            <div class="img-flex-r">
              <h2 class="home-des">Scout being cute and staring into your soul</h2>
              <div class="img-wrap home-imgs">
                <div class="img"></div>
              </div>
            </div>

          </section>
          <section class="home-img-sec">
            <div class="img-flex-l">
              <h2 class="home-des">Bailey winking in his pumpkin bandana next to his pumpkin</h2>
              <div class="img-wrap home-imgs">
                <div class="img"></div>
              </div>
            </div>
          </section>
        </section>


      </div>
      <div class="footer">
        <%- include("partials/footer"); -%>
      </div>
    </main>
    <script src="..\js\animations.js"></script>
  </body>
