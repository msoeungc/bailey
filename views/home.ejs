<%- include("partials/header"); -%>
<section class="color-sec">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-6">
        <h1 class="title-heading">See cute pictures of my dog, Bailey!</h1>
        <p class="title-content">
          <a href="/signup">Sign up</a> with your email to see more pictures of Bailey! (Example site)
        </p>
      </div>
      <div class="col-lg-6">
        <img class="bailsface-img" src="/images/bailsface.jpg" alt="bails-face">
      </div>
    </div>
  </div>
</section>

<section class="nocolor-sec carousel-sec">
  <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img src="/images/bailsmodel.jpg" class="carousel-img" alt="...">
        <h2 class="text-dark">Bailey Model</h2>
      </div>
      <div class="carousel-item">
        <img src="/images/bailsChristmas.jpg" class="carousel-img" alt="...">
        <h2 class="text-dark">Bailey Christmas</h2>
      </div>
      <div class="carousel-item">
        <img src="/images/bailsGarden.jpg" class="carousel-img" alt="...">
        <h2 class="text-dark">Bailey Garden</h2>
      </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</section>

<section class="color-sec">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <h1 class="about-header">More Photos!</h1>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-4">
        <img class="home-imgs" src="/images/bailsPool.jpg">
      </div>
      <div class="col-lg-4">
        <img class="home-imgs" src="/images/bailsCar.jpg">
      </div>
      <div class="col-lg-4">
        <img class="home-imgs" src="/images/bailsBlep.jpg">
      </div>
    </div>
    <div class="row">
      <div class="col-lg-4">
        <img class="home-imgs" src="/images/bailsSand.jpg">
      </div>
      <div class="col-lg-4">
        <img class="home-imgs" src="/images/bailsSmile.jpg">
      </div>
      <div class="col-lg-4">
        <img class="home-imgs" src="/images/bailsWink.jpg">
      </div>
    </div>
    <div class="row">
      <div class="col-lg-4">
        <img class="home-imgs" src="/images/bailsGroomed.jpg">
      </div>
      <div class="col-lg-4">
        <img class="home-imgs" src="/images/bailsEarsDown.jpg">
      </div>
      <div class="col-lg-4">
        <img class="home-imgs" src="/images/bailsBandana.jpg">
      </div>
    </div>
  </div>
</section>

<%- include("partials/footer"); -%>
